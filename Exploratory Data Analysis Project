        "id": "G3DFP1NauU9x"
      },
      "source": [
        "# Finding the number of goals conceded by summing the total # of goals conceded at home and away\n",
        "hometeamconceded = mostgoalsconceded.groupby('HomeTeam',as_index = False)['FTAG'].sum().sort_values(by = 'FTAG', ascending=True)\n",
        "awayteamconceded = mostgoalsconceded.groupby('AwayTeam',as_index = False)['FTHG'].sum().sort_values(by = 'FTHG', ascending=True)"
      ],
      "execution_count": null,
